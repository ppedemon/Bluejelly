module A
  (A.D(A.D,A.C,A.D)       -- D(..)
  ,A.C(A.C,A.op1,A.op2)   -- C(..)
  ,A.Arrow(A.arr)         -- *just* arr, *NOT* Arrow(arr)
  ,M.Color(M.Color,M.Red,M.Black)
  ,A.id
  ,A.Point(A.Point)
  ) where

data D a = C a | D a

data Point a = P (a,a)

class C a where
  op1 :: a -> a
  op2 :: a -> a

class Arrow a where
  arr :: a b c -> a b c -> a b c

id :: a -> a
